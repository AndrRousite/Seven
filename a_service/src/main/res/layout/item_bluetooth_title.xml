<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="wrap_content"
                                                   android:padding="@dimen/default_activity_16dp">

    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
              android:textSize="15sp"
              android:id="@+id/tvTitle"
              tools:text="QSPrinter_5956"
              app:layout_constraintTop_toTopOf="parent"
              app:layout_constraintBottom_toBottomOf="parent"
              android:textColor="@color/cl_666666" tools:ignore="MissingConstraints"/>


    <com.weyee.poswidget.layout.QMUIButton android:layout_width="70dp" android:layout_height="30dp"
                                           app:layout_constraintTop_toTopOf="parent"
                                           app:layout_constraintBottom_toBottomOf="parent"
                                           app:layout_constraintRight_toRightOf="parent"
                                           android:id="@+id/tvSearch"
                                           tools:text="搜索"
                                           android:gravity="center"
                                           android:textColor="@color/cl_50A7FF"
                                           app:qmui_radius="3dp"
                                           app:qmui_borderWidth="1dp"
                                           app:qmui_borderColor="@color/cl_50A7FF"
                                           android:background="@null"
                                           tools:ignore="MissingConstraints"/>

</androidx.constraintlayout.widget.ConstraintLayout>